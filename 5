a1,b1,c1 = map(int, input().split())
a2,b2,c2 = map(int, input().split())
det = a1*b2 - a2*b1
if det != 0:
    d = a1/a2
    y = (c2*d)/(b2*d)
    x = (c1-y*b1)/a1
    print("x = " + str(x) + ",y = " + str(y))
else:
    if a1/a2 == b1/b2 == c1/c2:
        print("VO SO NGHIEM")
    elif a1/a2 == b1/b2 != c1/c2:
        print("VO NGHIEM")